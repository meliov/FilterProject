<Window x:Class="FrontEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrontEnd"
        mc:Ignorable="d"
        Title="MainWindow" Height="861" Width="1212">
    <Grid Margin="0,0,0,13">
        <ComboBox ItemsSource="{Binding EntityNames}" HorizontalAlignment="Left" Margin="80,81,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox ItemsSource="{Binding Properties}" HorizontalAlignment="Left" Margin="325,80,0,0" VerticalAlignment="Top" Width="194" Height="22"/>
        <ComboBox ItemsSource="{Binding FilterActions}" HorizontalAlignment="Center" Margin="0,80,0,0" VerticalAlignment="Top" Width="88"/>
        <TextBox HorizontalAlignment="Left" Margin="691,80,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="187" Height="20"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="897,80,60,10"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,229,320,10"/>
        <Button Content="Remove Filter" HorizontalAlignment="Center" Margin="0,144,0,0" VerticalAlignment="Top" Width="172"/>
        <Button Content="Fetch Entries" HorizontalAlignment="Center" Margin="0,185,0,0" VerticalAlignment="Top" Width="172"/>
        <Button Content="Add Filter" HorizontalAlignment="Center" Margin="0,107,0,0" VerticalAlignment="Top" Width="172"/>
        <Label Content="Entities" HorizontalAlignment="Left" Margin="80,49,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Properties" HorizontalAlignment="Left" Margin="325,44,0,0" VerticalAlignment="Top" Width="180"/>
        <Label Content="Filter Action" HorizontalAlignment="Left" Margin="562,49,0,0" VerticalAlignment="Top" Width="73"/>
        <Label Content="Filter Value" HorizontalAlignment="Left" Margin="691,49,0,0" VerticalAlignment="Top" Width="69"/>
        <Label Content="Applied Filters" HorizontalAlignment="Left" Margin="897,49,0,0" VerticalAlignment="Top" Width="73"/>
        <Label Content="Filtered Entities" HorizontalAlignment="Left" Margin="253,179,0,0" VerticalAlignment="Top" Width="93"/>
    </Grid>
</Window>
